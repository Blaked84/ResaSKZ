%h1
  Product
  =@evenement

%table.striped.alternate
  %thead
    %tr
      %th Produit
      %th Quantitée totale
      %th Prix (€)
      %th Date limite de réservation
    %tbody
      - @product.each do |prd|
        %tr
          %td=link_to prd.name, productshow_path(prd.id)
          %td=prd.stock
          %td=prd.price_euro
          %td=prd.echeance


